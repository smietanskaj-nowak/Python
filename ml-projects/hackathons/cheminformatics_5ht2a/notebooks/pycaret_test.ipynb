import pycaret
from pycaret.classification import *

train = pd.concat([X_train, y_train], axis=1)
valid = pd.concat([X_val, y_val], axis=1)
test = pd.concat([X_test, y_test], axis=1)
# Set column as category (for classifciation purposes)
train['is_active'] = (train['pKi'] > 7).astype(int)
valid['is_active'] = (valid['pKi'] > 7).astype(int)
test['is_active'] = (test['pKi'] > 7).astype(int)

full = pd.concat([train, valid], axis=0).drop(columns=['pKi'])

# Set target
target = 'is_active'
features = [col for col in train.columns if col != target]

# AutoML initialization
aml = setup(
    data=full,
    target='is_active',
    session_id=42,
    silent=True,
    verbose=False
)


# AutoML training
best_model = compare_models()

# Model evaluation
evaluate_model(best_model)

# Prediction on test dataset
predictions = predict_model(best_model, data=test)

# Raport
from sklearn.metrics import classification_report
print(classification_report(test['isActive'], predictions['Label'], target_names=['inactive', 'active']))
